if(NOT EXISTS "/home/phenom/MediaSonic/build/install_manifest.txt")
    message(FATAL_ERROR "Cannot find install manifest: /home/phenom/MediaSonic/build/install_manifest.txt")
endif()
file(READ "/home/phenom/MediaSonic/build/install_manifest.txt" files)
string(REPLACE "\n" ";" files "${files}")
foreach(file ${files})
    if(EXISTS "${file}")
        message(STATUS "Removing ${file}")
        file(REMOVE "${file}")
    endif()
endforeach() 
